.model ma*mc
.inputs u v
.outputs Acc

.mv u 2 u1 u2
.mv v 2 v1 v2
.mv CS, NS  7  s0 s1 s2 s3 s4 s5 DC

.latch NS CS
.reset CS
s0

.table CS ->Acc
.default 1
(s1,s2,s4) 0

.table u v CS ->NS
.default DC
u1 v1   s0  s1
u1 v1   s1  s2
u2 v1   s1  s3
u1 v1   s2  s2
u2 v1   s2  s4
u2 v1   s3  s5
u2 v1   s4  s4
u2 v1   s5  s3

.end
