.model fig3ma*fig3mc
.inputs u v
.outputs Acc

.mv u 2 u1 u2
.mv v 2 v1 v2
.mv CS, NS  4  s0 s1 s3 DC1

.latch NS CS
.reset CS
s0

.table CS ->Acc
.default 1
DC1 0

.table u v CS ->NS
.default DC1
u1 v1    s0   s1
u1 v2    s0   s1
u2 v2    s0   s3
u2 v1    s0   s1
- -    s1   s1
u1 v1    s3   s1
u2 v2    s3   s1

.end
