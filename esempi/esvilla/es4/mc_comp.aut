.model es3mc
.inputs i o
.outputs Acc

.mv i 2 i1 i2
.mv o 2 o1 o2
.mv CS, NS  7  1 2 3 4 5 6 DC

.latch NS CS
.reset CS
1

.table CS ->Acc
.default 0
DC 1

.table i o CS ->NS
.default DC
i1 o1    1   4
i2 o1    1   2
i2 o2    2   1
i1 o2    2   3
i2 o1    3   2
i1 o2    3   6
i2 o2    4   1
i1 o2    4   5
- o2    5   1
- o1    6   2

.end
