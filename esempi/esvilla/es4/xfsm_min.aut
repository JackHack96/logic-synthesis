.model es43ma*es3mc
.inputs u o
.outputs Acc

.mv u 2 u1 u2
.mv o 2 o1 o2
.mv CS, NS  8  s0 s1 s3 s5 s6 s8 DC DC1

.latch NS CS
.reset CS
s0

.table CS ->Acc
.default 1
DC1 0

.table u o CS ->NS
.default DC1
u1 o1    s0   s1
u2 -    s0   DC
u1 o2    s1   s3
u2 o2    s1   s5
u1 o1    s3   s1
u2 o2    s3   s6
u1 o2    s5   s0
u2 -    s5   DC
u1 o1    s6   s8
u2 -    s6   DC
u1 o2    s8   s3
u2 -    s8   DC
- -    DC   DC

.end
