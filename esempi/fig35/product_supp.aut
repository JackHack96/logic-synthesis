.model fig35ma*fig35mc
.inputs u v
.outputs Acc

.mv u 2 u1 u2
.mv v 2 v1 v2
.mv CS, NS  8  s0 s1 s2 s3 s4 s5 s6 s7

.latch NS CS
.reset CS
s0

.table CS ->Acc
.default 0
(s1,s4,s5) 1

.table u v CS ->NS
u1 v1   s0  s1
u1 v1   s0  s2
- v2   s0  s3
- v2   s0  s4
u1 v1   s1  s1
u1 v1   s1  s5
- v2   s1  s4
u2 v1   s2  s6
- -   s2  s7
- -   s2  s4
- -   s3  s7
- -   s3  s4
- -   s4  s4
u2 v1   s5  s5
- -   s5  s4
u2 v1   s6  s2
- -   s6  s3
- -   s6  s4
- -   s7  s3
- -   s7  s4

.end
