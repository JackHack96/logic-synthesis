	findfont exch
	scalefont setfont
} def

/arrowhead {
	/arrowwidth exch def
	/arrowlength exch def
	gsave
		3 1 roll
		translate
			rotate
			newpath
			arrowlength arrowwidth 2 div moveto
			0 0 lineto
			arrowlength arrowwidth -2 div lineto
		closepath fill
		stroke
	grestore
